<h1>Viewing Party</h1>

<%= link_to "Register as a User", new_user_path %>
<%= link_to "I already have an account", login_path %>


<h2>Existing Users</h2>
<ul id="existing_users">
  <% @users.each do |user| %>
      <li><%= link_to user.email, "users/#{user.id}" %></li>
  <% end %>
</ul>

<%= button_to 'Create New User', register_user_path, method: :get, class: "btn btn-lg btn-outline-secondary" %><br>